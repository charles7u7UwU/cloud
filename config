upstream miapp{
         server WEBSERVER.vclass.local; 
         server aws.vlcass.local;
}
server {
        listen	80 default_server;
        listen	[::]:80 default_server; 
        server_name _;
        access_log /var/log/nginx/access_default_log;
        error_log /var/log/nginx/error_default_log;
location / {
        proxy_pass	http://miapp/cloud; 
        proxy_set_header X-Real-IP $remote_addr;

}
}
